最佳答案 #include <stdio.h>
#include <string.h>

char word[20]; //用于存储文件中读入的单词
int digit=0; //记录单词的位数

void READ_FILE(char read_text[]); //读文件函数
void Decode(FILE *fp); //解密函数，将文本中的单词一个个读出
void Transform(char write_text[]); //按题目要求转换记录单词，含THE及其变形不输出，其他单词则输出
void Reset(); //将word数组每个元素赋值为'\0'

void READ_FILE(char read_text[])
{
 FILE *read;
 read = fopen(read_text,"rt"); //打开读入单词的文档
 if (read == NULL)
 {
  printf("open file error\n");
  return;
 }
 Decode(read); //将文件指针传给加密函数
 fclose(read); //关闭文档
}

void Decode(FILE *fp)
{
 char ch; //记录读入字符
 while (ch != EOF) //判断是否读到文档结尾
 {
  ch = fgetc(fp); //在文档中读取字符
  if (ch == ' ') Transform("new.txt"); //如果读到空格说明读完一个单词，转入变形函数进行处理
  else word[digit++] = ch; //将读入字符保存到word字符数组中
 }
}

void Transform(char write_text[])
{
 FILE *write; 
 write = fopen(write_text,"a"); //以追加方式打开写入文件
 if (write == NULL)
 {
  printf("open file error");
  return;
 }
 if (strcmp(word,"The") != 0 && strcmp(word,"the") != 0) //判断该单词是否是THE或者其变形
 {
  if (word[0] < 'z' && word[0] > 'a' ) //判断单词首字母是否为小写
   word[0] = word[0] - 'a' + 'A'; //将小写首字母转换为大写
  fprintf(write,"%s\n",word); //在输出文档中写入该单词
 }
 digit = 0; //单词位数清零
 Reset(); //word[]字符数组清零
 fclose(write); //关闭写入文档
}

void Reset()
{
 for (int i = 0;i < 20; i++) word[i] = '\0'; //将word[]字符数组清零
}

void main()
{
 Reset();
 READ_FILE("org.dat");
}

